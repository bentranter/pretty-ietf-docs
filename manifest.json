{
  "name": "Pretty IETF Docs",
  "description": "Better typography for the IETF docs.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*.ietf.org/*"
      ],
      "css": ["ietf.css"]
    }
  ],
  "browser_action": {
    "default_title": "Pretty IETF Docs"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
